{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/trial", "destination": "/trial.html" },
    { "source": "/free-trial", "destination": "/trial.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "DENY" },
        { "key": "X-Content-Type-Options", "value": "nosniff" }
      ]
    }
  ]
}
